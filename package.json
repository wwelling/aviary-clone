{
  "name": "axiom-web",
  "version": "0.0.1",
  "description": "Axiom Web Interface",
  "author": "William Welling <william@axiominfo.tech>",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "ng": "ng",
    "rimraf": "rimraf",
    "prestart": "npm run build:axiom-elements",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "pretest": "npm run build:axiom-elements",
    "test": "ng test",
    "clean": "rimraf ./dist && rimraf ./logs",
    "predevelop": "npm run clean",
    "develop": "concurrently \"npm run build:axiom-elements:watch\" \"wait-on ./dist/axiom-elements/public-api.d.ts && ng serve --watch\"",
    "prebuild:axiom-components:watch": "npm run clean",
    "build:axiom-components:watch": "concurrently \"npm run build:axiom-elements:watch\" \"wait-on ./dist/axiom-elements/public-api.d.ts && ng build axiom-components --watch\"",
    "develop:axiom-components": "concurrently \"npm run build:axiom-components:watch\" \"wait-on ./dist/axiom-components/main.js && npm run dev:ssr\"",
    "dev:ssr": "ng run axiom-web:serve-ssr",
    "preserve:ssr": "npm run build:axiom-components",
    "serve:ssr": "node dist/axiom-web/server/main.js",
    "prebuild:ssr": "npm run build:axiom-components && ngcc",
    "build:ssr": "ng build && ng run axiom-web:server",
    "prerender": "ng run axiom-web:prerender",
    "build:axiom-elements": "ng build axiom-elements",
    "build:axiom-elements:watch": "ng build axiom-elements --watch",
    "prebuild:axiom-components": "npm run build:axiom-elements",
    "build:axiom-components": "ng build axiom-components"
  },
  "dependencies": {
    "@angular/animations": "^14.2.9",
    "@angular/common": "^14.2.9",
    "@angular/compiler": "^14.2.9",
    "@angular/core": "^14.2.9",
    "@angular/elements": "^14.2.9",
    "@angular/forms": "^14.2.9",
    "@angular/platform-browser": "^14.2.9",
    "@angular/platform-browser-dynamic": "^14.2.9",
    "@angular/platform-server": "^14.2.9",
    "@angular/router": "^14.2.9",
    "@nestjs/common": "^8.4.7",
    "@nestjs/core": "^8.4.7",
    "@nestjs/ng-universal": "^6.1.0",
    "@nestjs/platform-express": "^8.4.7",
    "@nestjs/serve-static": "^2.2.2",
    "@nguniversal/express-engine": "^14.2.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "env-var": "^7.3.0",
    "express": "^4.18.2",
    "nest-winston": "^1.8.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "~7.5.7",
    "tslib": "^2.4.1",
    "winston": "^3.8.2",
    "winston-daily-rotate-file": "^4.7.1",
    "zone.js": "~0.11.8"
  },
  "peerDependencies": {
    "fastify-static": "^4.7.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^14.2.8",
    "@angular/cli": "~14.2.8",
    "@angular/compiler-cli": "^14.2.9",
    "@nguniversal/builders": "^14.2.1",
    "@types/express": "^4.17.14",
    "@types/jasmine": "~4.3.0",
    "@types/node": "^18.11.9",
    "concurrently": "^7.5.0",
    "jasmine-core": "~4.5.0",
    "karma": "~6.4.1",
    "karma-chrome-launcher": "~3.1.1",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.0.0",
    "ng-packagr": "^14.2.2",
    "ngx-build-plus": "^14.0.0",
    "rimraf": "^3.0.2",
    "typescript": "~4.8.4",
    "wait-on": "^6.0.1"
  }
}
